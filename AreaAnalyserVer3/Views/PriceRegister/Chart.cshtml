@model AreaAnalyserVer3.Models.PriceRegister

@{
    ViewBag.Title = "Chart";
}

@section Amcharts {
    @Scripts.Render("https://www.amcharts.com/lib/3/amcharts.js")
    @Scripts.Render("https://www.amcharts.com/lib/3/serial.js")
    @Scripts.Render("https://www.amcharts.com/lib/3/plugins/export/export.min.js")
    @Scripts.Render("https://www.amcharts.com/lib/3/themes/dark.js")
    @Scripts.Render("~/Scripts/historyChart.js")
}

<h2>Property Price Register History</h2>

<div class="row">
    <div class="col-sm-8">
        <div id="chartdiv" style="width: 800px; height: 600px"></div>

    </div>
</div>

<script>
    $(function () { generateChart() });
</script>
